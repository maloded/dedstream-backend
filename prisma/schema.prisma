generator client {
  provider = "prisma-client"
  output   = "./generated"
}

datasource db {
  provider = "postgresql"
}

model User {
  id          String @id @default(uuid())
  email       String @unique
  password    String
  username    String @unique
  displayName String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
